	<script type="text/javascript" src="https://img.zecaifu.com/js/common.js?v=1.2"></script>
    <script type="text/javascript" src="https://www.zecaifu.com/js/detail.js"></script>
    <script type="text/javascript" src="https://www.zecaifu.com/js/jquery.blockUI.js"></script>
    <script>
        $(document).ready(function() {
            ajax_show(1);
            ajax_message(1);
        });

        function ajax_show(p) {
            $.get("/detail/6462/list?page="+p, function(data){
                $("#J-c-record").html(data);
                $("#investor_list").find('a').click(function(){
                    var _num=$(this).attr('data');
                    ajax_show(_num);
                });
            });
        }
        function ajax_message(p){

            $.get("/detail/6462/message?page="+p, function(data){
                $("#message").html(data);
                $("#message_list").find('a').click(function(){
                    var _num=$(this).attr('data');
                    ajax_message(_num);
                });
            });
        }
        function checkLength(obj) {
            var curr =  obj.value.length;
            $("#checklen").html(curr.toString());
        }
        $("#J-btn-send").click(function () {
            var message_content=$("#message-content").val();
            var borrowName = "第4180期-宝马740LI";
            if(message_content.length>0){
                $.ajax({
                    type:'post',
                    url:"/detail/6462/message",
                    data:{comment:message_content,borrowName:borrowName,_token:$("input:hidden").attr("value")},
                    dataType:'json',
                    headers:{ 'X-CSRF-TOKEN': $('._token').val()},
                    success:function(data){
                        if(data.status==2){
                            $('#message-content').val('');
                            fun_popup_show_001('系统提示',data.msg,'fun_popup_close_001();ajax_message(1);','formSubmit');
                        }else if(data.status==0){
                            fun_popup_show_001('系统提示',data.msg,'https://www.zecaifu.com/login','url')
                        }else if(data.status==1){
                            fun_popup_close_001('系统提示',data.msg,'fun_popup_close_001();','formSubmit')
                        }else{

                        }

                    }
                })
            }
        })
        $("#investor_button").click(function(){
            var num = $("#J-buy-num").val();
            if(num>0){
                $.ajax({
                    type:'post',
                    url:"/detail/NqcirS3D",
                    data:{num:num ,_token:$("input:hidden").attr("value")},
                    dataType:'json',
                    headers:{ 'X-CSRF-TOKEN': $("input:hidden").attr("value")},
                    success:function(data){
                        if(data.status==0){
                            fun_popup_show_001('系统提示',data.msg,'https://www.zecaifu.com/login','url')
                        }else if(data.status==1){
                            fun_popup_show_001('系统提示',data.msg,'fun_popup_close_001();','formSubmit')
                        }else if(data.status==3){
                            fun_popup_show_001('系统提示',data.msg,'https://www.zecaifu.com/profile/recharge','url')
                        }else if(data.status==2){
                            location.href=data.msg;
                        }else if(data.status==4){
                            fun_popup_show_001('系统提示',data.msg,'https://www.zecaifu.com/profile/setting','url')
                        }
                    }
                })
            }
        })
        $("#agree-btn").click(function(){
            vote(1,"6462");
        })
        $("#disagree-btn").click(function(){
            vote(2,"6462");
        })
        function vote(agree,bid){
            $.ajax({
                type:'post',
                url:"/detail/vote",
                data:{agree:agree ,bid:bid,_token:$("input:hidden").attr("value")},
                dataType:'json',
                headers:{ 'X-CSRF-TOKEN': $("input:hidden").attr("value")},
                success:function(data){
                    if(data.status==0){
                        fun_popup_show_001('系统提示',data.msg,'fun_popup_close_001();','formSubmit')
                    }else if(data.status==1){
                        $("#agree-num").html(data.vote_num.agree);
                        $("#disagree-num").html(data.vote_num.disagree);
                        fun_popup_show_001('系统提示',data.msg,'fun_popup_close_001();','formSubmit')
                    }
                }
            })
        }
        function reset_num(data){
            $("#agree-num").html(data.agree);
            $("#disagree-num").html(data.disagree);
        }
        function OnInput(){
            var per_transfer = 100
                    true_money=$("#J-buy-num").val()*per_transfer;
            $("#true_money").html(true_money);
        }
        function OnPropChanged (){
            var per_transfer = 100
                    true_money=$("#J-buy-num").val()*per_transfer;
            $("#true_money").html(true_money);
        }
    </script>
</body></html>
